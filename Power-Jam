# Tool Name :- Power-Jam
# Creator :- Lesego Ruele
# Function :- Access other tools
# Created :- 5/07/2020

if [ -d /data/data/com.termux/files/usr/etc ]; then
  conf_dir="/data/data/com.termux/files/usr/etc"
elif [ -d /usr/etc ]; then
  conf_dir="/usr/etc"
elif [ -d /etc ]; then
  conf_dir="/etc"
fi

if [ -e /usr/lib/sudo ]; then
	sudo python3 $conf_dir/Power-Jam
else
	python3 $conf_dir/Power-Jam
fi
exit
